<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<LangVersion>latest</LangVersion>
		<Nullable>enable</Nullable>
		<WarningsAsErrors>nullable</WarningsAsErrors>
		<Version>1.0.5-beta</Version>
	</PropertyGroup>

	<ItemGroup>
	  <PackageReference Include="MessagePack" Version="2.5.124" />
	  <PackageReference Include="MessagePackAnalyzer" Version="2.5.124">
	    <PrivateAssets>all</PrivateAssets>
	    <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
	  </PackageReference>
	  <PackageReference Include="MetaFac.CG4.Runtime" Version="2.0.2" />
	  <PackageReference Include="MetaFac.CG4.Runtime.MessagePack" Version="2.0.2" />
	  <PackageReference Include="MetaFac.Memory" Version="1.2.3" />
	  <PackageReference Include="MetaFac.Mutability" Version="2.1.4" />
	</ItemGroup>

</Project>